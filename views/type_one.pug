html
  head
   style
     include ./style.css
   title= "Type I"
  a(href='/') Go to start
  h1 Client: #{clientName}
  p
    a(href='/results') Summary
    a(class='selected' href='/result_type_one') Type I
    a(href='/result_type_two') Type II
    a(href='/result_type_three') Type III
    a(href='/result_failed') Failed
  if records > 0
    a(class='download' href='/download?filename=typeOne') Download
    table
      tr
          th Url
          th DR
          th TR
      each x in xs
        tr
          td  #{x.url}
          td  #{x.dr}
          td  #{x.tr}
  else
    div No records
