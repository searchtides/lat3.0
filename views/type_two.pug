html
  head
    style
      include ./style.css
    title= "Type II"
  a(href='/') Go to start
  h1 Client: #{clientName}
  p
    a(href='/summary') Summary
    a(href='/result_type_one') Type I
    a(class='selected' href='/result_type_two') Type II
    a(href='/result_type_three') Type III
    a(href='/result_failed') Failed
  if records > 0
    a(class='download' href='/download?filename=typeTwo') Download
    table
      tr
          th Url
          th DR
          th TR
          th US Traffic
          th Write for us
          th Spam
          th Trend angle
          th Keywords
 
      each x in xs
        tr
          td  #{x.url}
          td  #{x.dr}
          td  #{x.tr}
          td  #{x.us_tr}
          td  #{x.writeToUs}
          td  #{x.spam}
          td  #{x.angle}
          td  #{x.kwds}
  else
    div No records
